<odoo>
    <record id="view_trainer_report_wizard_form" model="ir.ui.view">
        <field name="name">trainer.report.wizard.form</field>
        <field name="model">trainer.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Trainer Report Wizard">
                <group>
                    <field name="trainer_id"/>
                    <field name="from_date"/>
                    <field name="to_date"/>
                </group>
                <footer>
                    <button name="action_print"
                            string="Print"
                            type="object"
                            class="btn-primary"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
